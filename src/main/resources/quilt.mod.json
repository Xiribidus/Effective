			},
			{
				"id": "minecraft",
				"versions": ">=1.19.3"
			},
			{
				"id": "lodestone",
				"versions": "*"
			}
		]
	},
	"mixin": [
		"effective.mixins.json",
		"illuminations.mixins.json"
	],
	"access_widener": [
		"effective.accesswidener",
		"illuminations.accesswidener"
	]
