{
	"schema_version": 1,
	"quilt_loader": {
		"group": "ladysnake",
		"id": "effective",
		"version": "${version}",
		"metadata": {
			"name": "Effective",
			"description": "A client-side mod adding ambient and environmental interaction effects to enhance immersion.",
			"contributors": {
				"doctor4t": "Owner",
				"Pyrofab": "Contributor",
				"lonefelidae16": "Contributor"
			},
			"contact": {
				"homepage": "https://doctor4t.ladysnake.org/",
				"sources": "https://github.com/Ladysnake/Effective",
				"issues": "https://github.com/Ladysnake/Effective/issues"
			},
			"icon": "assets/effective/effective.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
